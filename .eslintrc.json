{
    "env": {
        "es6": true,
    "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
    },
    "plugins": [
        "prettier"
    ],
    "rules": {
    "prettier/prettier": "error",
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "camelcase": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }],
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "no-console": "off",
    "import/prefer-default-export": "off"
    }
}
